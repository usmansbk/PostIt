{
  "name": "postit",
  "version": "1.0.0",
  "description": "Minimalist social network web app",
  "main": "server.js",
  "scripts": {
    "build": "sh server/scripts/build.sh",
    "start:server": "node server/build/bin/www",
    "start:dev": "nodemon server/build/bin/www",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=server/tests/spec/support/jasmine.json",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usmansbk/PostIt.git"
  },
  "keywords": [
    "PostIt",
    "Social Network",
    "Node",
    "Sequelize",
    "Postgres",
    "React",
    "Redux"
  ],
  "author": "Babakolo Usman Suleiman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/usmansbk/PostIt/issues"
  },
  "homepage": "https://github.com/usmansbk/PostIt#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "morgan": "^1.9.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "request": "^2.83.0",
    "sequelize": "^4.28.6",
    "sequelize-cli": "^3.2.0"
  },
  "nyc": {
    "exclude": [
      "**/*[sS]pec.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jasmine": "^2.8.0",
    "nodemon": "^1.14.3",
    "nyc": "^11.4.1"
  }
}
